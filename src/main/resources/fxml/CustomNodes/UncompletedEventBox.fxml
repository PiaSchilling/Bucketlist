<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="anchorpane" prefHeight="100.0" prefWidth="970.0" xmlns="http://javafx.com/javafx/14.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <CheckBox id="checkBox" fx:id="checkBox" layoutX="33.0" layoutY="40.0" mnemonicParsing="false" onAction="#completeEvent" scaleX="2.0" scaleY="2.0" styleClass="checkBox">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </CheckBox>
      <Label id="eventNameLabel" fx:id="eventNameLabel" layoutX="77.0" layoutY="25.0" styleClass="h2" text="Eventname" />
      <Button id="deleteButton" fx:id="deleteButton" layoutX="898.0" layoutY="27.0" mnemonicParsing="false" onAction="#deleteEvent" prefHeight="47.0" prefWidth="48.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView id="deleteIcon" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/Dark_Design/Icons_White/more_vert_white.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label id="categoryLabel" fx:id="categoryLabel" layoutX="78.0" layoutY="54.0" styleClass="h3" stylesheets="@../../styles/CustomNodes/UncompletedEventBox.css" text="Category" />
   </children>
   <stylesheets>
      <URL value="@../../styles/CustomNodes/UncompletedEventBox.css" />
      <URL value="@../../styles/general.css" />
   </stylesheets>
</AnchorPane>
