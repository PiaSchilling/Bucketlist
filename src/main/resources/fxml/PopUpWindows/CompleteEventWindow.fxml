<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="274.0" prefWidth="522.0" styleClass="anchorpane" stylesheets="@../../styles/PopUpWindows/popUpWindow.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="dayComboBox" layoutX="390.0" layoutY="58.0" prefHeight="27.0" prefWidth="95.0" promptText="day" styleClass="combo-box">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="monthComboBox" layoutX="390.0" layoutY="112.0" prefHeight="27.0" prefWidth="95.0" promptText="month" styleClass="combo-box">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="yearComboBox" layoutX="390.0" layoutY="167.0" prefHeight="27.0" prefWidth="95.0" promptText="year" styleClass="combo-box">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <Label layoutX="391.0" layoutY="28.0" text="pick a date" />
      <TextArea fx:id="textarea" layoutX="22.0" layoutY="119.0" prefHeight="75.0" prefWidth="313.0" wrapText="true" />
      <Label layoutX="22.0" layoutY="97.0" text="describe your experience (optional)" />
      <Button fx:id="selectImageButton" layoutX="22.0" layoutY="51.0" mnemonicParsing="false" onAction="#selectImage" text="select">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label layoutX="22.0" layoutY="28.0" prefHeight="17.0" prefWidth="215.0" text="select an image (optional)" />
      <Button fx:id="completeButton" layoutX="214.0" layoutY="226.0" mnemonicParsing="false" onAction="#completeEvent" text="complete">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label fx:id="errorLabel" layoutX="22.0" layoutY="203.0" prefHeight="17.0" prefWidth="403.0" textFill="#ffbc81" />
   </children>
</AnchorPane>
