<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="405.0" prefWidth="361.0" styleClass="anchorpane" stylesheets="@../../styles/PopUpWindows/popUpWindow.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="dayComboBox" layoutX="24.0" layoutY="259.0" prefHeight="27.0" prefWidth="95.0" promptText="day" styleClass="combo-box">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="monthComboBox" layoutX="133.0" layoutY="260.0" prefHeight="27.0" prefWidth="95.0" promptText="month" styleClass="combo-box">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="yearComboBox" layoutX="241.0" layoutY="260.0" prefHeight="27.0" prefWidth="95.0" promptText="year" styleClass="combo-box">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <Label layoutX="24.0" layoutY="237.0" text="pick a date" />
      <TextArea fx:id="textarea" layoutX="23.0" layoutY="146.0" prefHeight="75.0" prefWidth="313.0" wrapText="true" />
      <Label layoutX="23.0" layoutY="122.0" text="describe your experience (optional)" />
      <Button fx:id="selectImageButton" layoutX="24.0" layoutY="82.0" mnemonicParsing="false" onAction="#selectImage" text="select">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label layoutX="24.0" layoutY="59.0" prefHeight="17.0" prefWidth="215.0" text="select an image (optional)" />
      <Button fx:id="completeButton" layoutX="140.0" layoutY="356.0" mnemonicParsing="false" onAction="#completeEvent" text="complete">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="27.0" layoutY="312.0" prefHeight="17.0" prefWidth="300.0" textFill="#ffbc81" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="35.0" prefWidth="38.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/Dark_Design/Icons_White/backwards.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
