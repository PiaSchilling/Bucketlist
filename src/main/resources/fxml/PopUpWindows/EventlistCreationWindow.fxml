<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="243.0" prefWidth="565.0" styleClass="anchorpane" stylesheets="@../../styles/PopUpWindows/popUpWindow.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="16.0" layoutY="16.0" text="Create a new Eventlist" />
      <Label layoutX="28.0" layoutY="66.0" text="Name" />
      <TextField fx:id="listNameField" layoutX="25.0" layoutY="93.0" promptText="please enter Eventlistname" />
      <Label layoutX="242.0" layoutY="62.0" text="Expiry Date (optional)" />
      <Button fx:id="createButton" layoutX="254.0" layoutY="184.0" mnemonicParsing="false" onAction="#createEventlist" text="create">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label fx:id="errorLabel" layoutX="24.0" layoutY="118.0" textFill="#8d1212" />
      <ComboBox fx:id="dayBox" layoutX="236.0" layoutY="93.0" prefHeight="27.0" prefWidth="82.0" promptText="day">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="monthBox" layoutX="335.0" layoutY="93.0" prefHeight="27.0" prefWidth="90.0" promptText="month">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="yearBox" layoutX="437.0" layoutY="94.0" prefHeight="27.0" prefWidth="82.0" promptText="year">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
   </children>
</AnchorPane>
