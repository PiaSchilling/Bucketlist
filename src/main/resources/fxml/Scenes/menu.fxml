<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="parent" prefHeight="800.0" prefWidth="1280.0" stylesheets="@../../styles/general.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <BorderPane fx:id="borderPane" prefHeight="800.0" prefWidth="1280.0">
         <left>
                <VBox prefHeight="611.0" prefWidth="203.0" BorderPane.alignment="CENTER">
                    <children>
                        <ImageView id="logoImage" fitHeight="112.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true">
                            <VBox.margin>
                                <Insets left="30.0" top="50.0" />
                            </VBox.margin>
                     <image>
                        <Image url="@../../images/Dark_Design/LogoBucketlist_white.png" />
                     </image>
                        </ImageView>
                        <Button id="listsButton" fx:id="viewListButton" mnemonicParsing="false" onAction="#showListScene" prefHeight="77.0" prefWidth="100.0" styleClass="menu" stylesheets="@../../styles/general.css">
                            <VBox.margin>
                                <Insets left="50.0" top="60.0" />
                            </VBox.margin>
                     <graphic>
                        <ImageView fitHeight="68.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/Dark_Design/Icons_White/menu_eventlists.png" />
                           </image>
                        </ImageView>
                     </graphic>
                        </Button>
                        <Button id="catButton" fx:id="viewCatButton" mnemonicParsing="false" onAction="#showCatScene" prefHeight="77.0" prefWidth="100.0" styleClass="menu" stylesheets="@../../styles/general.css">
                            <VBox.margin>
                                <Insets left="50.0" top="60.0" />
                            </VBox.margin>
                     <graphic>
                        <ImageView fitHeight="68.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/Dark_Design/Icons_White/menu_category.png" />
                           </image>
                        </ImageView>
                     </graphic>
                        </Button>
                        <Button id="statisticButton" fx:id="viewStatisticButton" mnemonicParsing="false" onAction="#showStatisticScene" prefHeight="77.0" prefWidth="100.0" styleClass="menu" stylesheets="@../../styles/general.css">
                            <VBox.margin>
                                <Insets left="50.0" top="60.0" />
                            </VBox.margin>
                     <graphic>
                        <ImageView fitHeight="68.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/Dark_Design/Icons_White/menu_statistics.png" />
                           </image>
                        </ImageView>
                     </graphic>
                        </Button>
                    </children>
                </VBox>
         </left>
      </BorderPane>
    </children>
</AnchorPane>
